.content-buy-sel {
	width:342px;
    height:40px;
    line-height: 40px;
    border: 1px solid #c5c6ca;
  display: inline-block;
  /* 1 */
  position: relative;
  /* 2 */
  vertical-align: middle;
  /* 3 */
  padding: 0;
  /* 4 */
  overflow: hidden;
  /* 5 */
  background: #fff;
  color: #555;
  text-shadow: none;
  -moz-border-radius: 1px;
  -webkit-border-radius: 1px;
  border-radius: 1px;
  -moz-transition: box-shadow 0.25s ease;
  -o-transition: box-shadow 0.25s ease;
  -webkit-transition: box-shadow 0.25s ease;
  transition: box-shadow 0.25s ease;
  /* 6 */
  /* 6 */
}
.content-buy-sel:hover {
  -moz-box-shadow: 0 1px 4px rgba(0, 0, 0, 0.15);
  -webkit-box-shadow: 0 1px 4px rgba(0, 0, 0, 0.15);
  box-shadow: 0 1px 4px rgba(0, 0, 0, 0.15);
}
.content-buy-sel:before {
  position: absolute;
  /* 7 */
  top: 1px;
  /* 7 */
  right: 0.5em;
  /* 7 */
  pointer-events: none;
  color: #b2b4bd;
  /* 8 */
}
.content-buy-sel select {
	width:342px;
    height:40px;
  cursor: pointer;
  /* 9 */
  padding: 10px;
  /* 10 */
  padding-right: 2em;
  /* 11 */
  /*width: 130%;*/
  /* 12 */
  border: none;
  /* 13 */
  background: transparent;
  /* 13 */
  background-image: none;
  /* 13 */
  -webkit-appearance: none;
  /* 13 */
  -moz-appearance: none;
  /* 13 */
  appearance: none;
  /* 13 */
  text-indent: 0.01px;
  /* 14 */
  text-overflow: '';
  /* 14 */
  /* 15 */
}
.content-buy-sel select:disabled {
	color: red;
}
.content-buy-sel select:focus {
  outline: none;
  /* 16 */
}

