.red-radio{
	opacity: 0;
	width: 12px;
    margin-right: -14px;
}
.red-radio + label{
	min-width: 50px;
	height: 42px;
	line-height: 42px;
	cursor: pointer;
	padding: 0 0 0 20px;
	margin-right: 20px;
	display: inline-block;
	position: relative;
}
.red-radio + label:before,
.red-radio + label:after{
	content: '';
	width: 10px;
	height: 10px;
	display: block;
	border: 1px solid #d2d2d5;
	border-radius: 100%;
	top: 0;
	left: 0;
	margin: 14px 0 0 2px;
	position: absolute;
}
.red-radio:checked + label:before{
	border-color: #fc401d;
}
.red-radio:checked + label:after{
	width: 8px;
	height: 8px;
	border: none;
	margin: 16px 0 0 4px;
	background: #fc401d;
}
