}
.rongyu_table,.licheng_table{
	width: 100%;
}
.rongyu_table tr,.licheng_table tr{
	height: 48px;
}
.rongyu_table tr:nth-child(odd), .licheng_table tr:nth-child(odd){
	background: #f5f5f8;
}
.rongyu_table tr td{
	font-size: 12px;
	text-align: left;
	padding-left: 18px;
}
.licheng_table tr td{
	text-align: center;
	font-size: 12px;
}
.licheng_table tr td.licheng_txt{
	text-align: left;
	padding-left: 46px;
}